<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER" prefHeight="35.0" prefWidth="200.0" spacing="40.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Arezzo.Controllers.NoteController">
   <children>
      <Label fx:id="octave" text="Octave" />
      <ImageView fx:id="image" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="note" alignment="CENTER" contentDisplay="CENTER" text="Note" textAlignment="CENTER" />
   </children>
</HBox>
